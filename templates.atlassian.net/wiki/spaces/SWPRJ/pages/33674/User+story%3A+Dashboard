        <h2 id="Userstory:Dashboard-Userinteractionanddesign">
          User interaction and design
        </h2>
        <div>
          <table>
            <colgroup>
              <col/>
              <col/>
            </colgroup>
            <tbody>
              <tr>
                <th>
                  Image
                </th>
                <th>
                  Description
                </th>
              </tr>
              <tr>
                <td>
                  <div>
                    <p>
                      <span>
                        <img height="460" width="268" src="https://templates.atlassian.net/wiki/download/attachments/33674/dashboard.png?api=v2">
                      </span>
                    </p>
                  </div>
                </td>
                <td>
                  <p>
                    Colors hex codes:
                  </p>
                  <ul>
                    <li>
                      <ul>
                        <li>
                          Blue header: 2362B9
                        </li>
                        <li>
                          Share icon: 5964EF
                        </li>
                        <li>
                          Mention icon: E83386
                        </li>
                        <li>
                          Task icon: A55DFB
                        </li>
                        <li>
                          Icons: 9CA7C2
                        </li>
                      </ul>
                    </li>
                  </ul>
                  <p>
                    Size:
                  </p>
                  <ul>
                    <li>
                      <ul>
                        <li>
                          Notification bar height: 50px
                        </li>
                        <li>
                          Navigation bar height: 40px
                        </li>
                        <li>
                          Avatar: 20x20px
                        </li>
                      </ul>
                    </li>
                  </ul>
                </td>
              </tr>
            </tbody>
          </table>
